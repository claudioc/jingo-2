    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.9",
    "@types/sinon": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "ava": "^3.15.0",
    "cheerio": "^1.0.0-rc.2",
    "cpy-cli": "^3.1.1",
    "editorconfig": "^0.15.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
