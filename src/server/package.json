    "memfs": "^2.6.0",
    "mock-fs": "^4.4.2",
    "mountfs": "^0.5.0",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2",
    "tmp": "0.0.33",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/passport": "^1.0.3",
    "body-parser": "^1.19.0",
    "commander": "^2.12.1",
